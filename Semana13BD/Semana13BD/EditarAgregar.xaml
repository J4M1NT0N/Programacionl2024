<Window x:Class="Semana13BD.EditarAgregar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Agregar/Editar Venta" Height="200" Width="300">
    <Grid>
        <StackPanel Margin="10">
            <TextBox x:Name="ProductoIdTextBox" Text="Producto Id" Margin="0,0,0,10"/>
            <TextBox x:Name="CantidadTextBox" Text="Cantidad" Margin="0,0,0,10"/>
            <DatePicker x:Name="FechaDatePicker" Margin="0,0,0,10"/>
            <Button Content="Guardar" Click="Guardar_Click"/>
        </StackPanel>
    </Grid>
</Window>
